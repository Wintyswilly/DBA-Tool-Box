                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">use DBREPORTS
SELECT *

FROM 
(select  target_name, 'MISSING DatabaseBackup - SYSTEM_DATABASES - FULL' MISSING
from dbreports.ConfigData.TargetInstances ti
WHERE target_name NOT IN (
SELECT SRVNAME FROM DBREPORTS.dbo.dbagentjobs
 WHERE JOBNAME in ('DatabaseBackup - SYSTEM_DATABASES - FULL')					
															   )
AND TI.KnowStatus = 'ACTIVE'
union
select  target_name, 'MISSING DatabaseBackup - USER_DATABASES - DIFF' MISSING
from dbreports.ConfigData.TargetInstances ti
WHERE target_name NOT IN (
SELECT SRVNAME FROM DBREPORTS.dbo.dbagentjobs
 WHERE JOBNAME in ('DatabaseBackup - USER_DATABASES - DIFF')															   )
AND TI.KnowStatus = 'ACTIVE'
union
select  target_name, 'MISSING DatabaseBackup - USER_DATABASES - FULL' MISSING
from dbreports.ConfigData.TargetInstances ti
WHERE target_name NOT IN (
SELECT SRVNAME FROM DBREPORTS.dbo.dbagentjobs
 WHERE JOBNAME in ('DatabaseBackup - USER_DATABASES - FULL')															   )
AND TI.KnowStatus = 'ACTIVE'
union
select  target_name, 'MISSING DatabaseBackup - USER_DATABASES - LOG' MISSING
from dbreports.ConfigData.TargetInstances ti
WHERE target_name NOT IN (
SELECT SRVNAME FROM DBREPORTS.dbo.dbagentjobs
 WHERE JOBNAME in ('DatabaseBackup - USER_DATABASES - LOG')															   )
AND TI.KnowStatus = 'ACTIVE'
union
select  target_name, 'MISSING DatabaseBackup - Netbackup tranact Trunc V2' MISSING
from dbreports.ConfigData.TargetInstances ti
WHERE target_name NOT IN (
SELECT SRVNAME FROM DBREPORTS.dbo.dbagentjobs
 WHERE JOBNAME in ('DatabaseBackup - Netbackup tranact Trunc V2')															   )
AND TI.KnowStatus = 'ACTIVE'
) A</property>
